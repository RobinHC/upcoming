FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint;

setFormat       raw;

sets
(
    axial
    {
        type    uniform;
        axis    x;
        start   (0 0.00000001 0);
        end     (0.254 0.00000001 0);
        nPoints 1000;
    }
);

fields          (U p);

