FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [1 -1 -2 0 0 0 0];
#include        "input/inputs"

internalField   uniform $initial_pressure;

boundaryField 
{
    inlet
    {
        type            totalPressure;
        p0              uniform $total_pressure;
        U               U;
        phi             phi;
        rho             rho;
        psi             none;
        gamma           1.4;
        value           uniform $total_pressure;
    }
    // outlet
    // {
    //     type            waveTransmissive;
    //     field           p;
    //     phi             phi;
    //     rho             rho;
    //     psi             thermo:psi;
    //     gamma           1.4;
    //     fieldInf        $exit_pressure;
    //     // lInf: A measure of how far away the far-field condition should be. 
    //     lInf            0.0254; 
    //     value           $internalField;
    // }
    outlet
    {
        type            fixedValue;
        value           uniform $exit_pressure;
    }

    nozzle
    {
        type            slip;
    }

    wedge0
    {
        type            wedge;
    }
    wedge1
    {
        type            wedge;
    }
}

